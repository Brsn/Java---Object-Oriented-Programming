import java.util.ArrayList;
import java.util.Arrays;
import java.util.list;



 public class PassingListAsArgument
{
  public static void main(String[] args) 
{

  String[] strings = { "hi", "hello", "howdy", "hey"}
  List<String> words = new ArrayList<>(Arrays.asList(strings));
  System.out.printf("wordCount(words, quit) .. %d%n", wordCount(words, "hello"));
}
  private static int wordCount(List<String> wordList, String word)
{
  int count = 0;
  for(String el: wordList)
  {
   if(el.equals(word))
   {
   count++;
     } 
}
   return count;
}
